<anf>

    <globals>
        <drawing mode="fill" shading="gouraud" background="0 0 0 0" />
        <culling face="front" order="cw" />
 
        <lighting doublesided="false" local="false" enabled="true" ambient="0.2 0.2 0.2 1" />
 
    </globals>
    
    <cameras initial="cam1" >
        <perspective id="cam1" near="0.1" far="500" angle="45" pos="12 5 12" target="5 0.75 5" />
        

    </cameras>

