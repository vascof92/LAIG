        <component type="diffuse" value="1.0 1.0 1.0 1" />
        <component type="specular" value="0.77 0.77 0.77 1" />
        
      </appearance>
      
      <appearance id="water" shininess="50" textureref="water">
        <component type="ambient" value="1.0 1.0 1.0 1" />
        <component type="diffuse" value="1.0 1.0 1.0 1" />
        <component type="specular" value="0.77 0.77 0.77 1" />
