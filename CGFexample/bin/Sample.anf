			</transforms>
			
			<appearanceref id="wood" />
			
			<primitives>  
				<cylinder base="20" top="5" height="1" slices="15" stacks="15" />
			</primitives>
		</node>
		
		<!-- Torus -->
		
		<node id="donut">
			<transforms>
				<transform type="translate" to="13.5 6.75 13.5" />
			</transforms>
			
			<appearanceref id="donut" />
			
			<primitives>  
				<torus inner="0.7" outer="1" slices="20" loops="20" />
			</primitives>
		</node>
	</graph>
</anf>