		</node>
		
		<!-- Table -->
		
		<node id="mesa">
			<transforms>
				<transform type="translate" to="10 0 10" />
			</transforms>
			
			<primitives>
			</primitives>
			
			<appearanceref id="metal" />
			
			<descendants>
				 <noderef id="mesa_perna_1" />
				 <noderef id="mesa_perna_2" />
				 <noderef id="mesa_perna_3" />
				 <noderef id="mesa_perna_4" />
				 
				 <noderef id="mesa_tapume" />
			 </descendants>
		</node>
		
		<node id="mesa_perna_1">
			<transforms>
				
			</transforms>
			
			<appearanceref id="inherit" />
			
			<primitives>  
				
			</primitives>
			
			<descendants>
				 <noderef id="perna_mesa" />
			 </descendants>
